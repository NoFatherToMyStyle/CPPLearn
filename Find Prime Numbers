#include "std_lib_facilities.h"

bool isPrime(int x) {
	bool check = true;
	if (x % 2 == 0 || x % 3 == 0 || x % 5 == 0 || x % 7 == 0) check = false;
	if (x == 1 || x == 2 || x == 3 || x == 5 || x == 7) check = true;
	return check;
}

int main()
{
	vector<int> primes;
	for (int i = 0; i <= 100; ++i) {
		if (isPrime(i) == true) primes.push_back(i);
	}
	for (int i = 0; i < primes.size(); ++i) {
		cout << primes[i] << "\n";
	}
	return 0;
